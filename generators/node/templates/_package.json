{
  "name": "<%= moduleName %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "license": "MIT",
  "repository": "https://github.com/<%= githubUsername %>/<%= moduleName %>.git",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>",
    "url": "<%= website %>"
  },
  <% if (cli) { %>
  "bin": {
    "<%= moduleName %>": "bin/<%= moduleName %>.js"
  },
  <% } %>
  "main": "lib/index.js",
  "engines": {
  <% if (es6) %>
    "node": ">=2.0.1"
  <% } else { %>
    "node": ">=0.10.0"
  <% } %>
  },
  "scripts": {
    "test": "grunt test"
  },
  "keywords": [
  <% if (cli) { %>
    "cli-app",
    "cli"
  <% } %>
  ],
  "dependencies": {
  <% if (cli) { %>
    "yargs": "^3.15.0"
  <% } %>
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "chai-as-promised": "^5.1.0",
    "eslint": "^0.24.0",
    "grunt": "^0.4.5",
    "grunt-bump": "^0.3.1",
    "grunt-cli": "^0.1.13",
    "grunt-dev-update": "^1.3.0",
    "grunt-eslint": "^16.0.0",
    "grunt-mocha-istanbul": "^2.4.0",
    "istanbul-harmony": "^0.3.16",
    "jit-grunt": "^0.9.1",
    "load-grunt-config": "^0.17.1",
    "mocha": "^2.2.5",
    "sinon": "^1.14.1",
    "sinon-chai": "^2.8.0",
    "time-grunt": "^1.2.1"
  }
}
